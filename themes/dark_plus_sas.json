{
	"$schema": "vscode://schemas/color-theme",
	"name": "Dark+ (SAS)",
	"include": "./dark_plus.json",
	"tokenColors": [
		{
			"scope": "keyword.control.macro.reversed",
			"settings": {
				"foreground": "#D775CF",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "keyword.control.macro.reversed.variable",
			"settings": {
				"foreground": "#D775CF",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"keyword.data.sas",
				"keyword.proc.sas",
				"keyword.quit.sas",
				"keyword.run.sas"
			],
			"settings": {
				"foreground": "#569cd6",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"keyword.operator.logical.literal.sas",
				"keyword.other.proc.sql.operator.logical.sas"
			],
			"settings": {
				"foreground": "#C586C0"
			}
		},
		{
			"scope": "string.unquoted",
			"settings": {
				"foreground": "#CEBC78"
			}
		},
		{
			"scope": [
				"support.type.format-informat",
				"support.type.format",
				"support.type.informat"
			],
			"settings": {
				"foreground": "#AECC61"
			}
		},
		{
			"scope": "support.type.format-informat.custom",
			"settings": {
				"foreground": "#81CC61"
			}
		},
		{
			"scope": "variable.macro.function",
			"settings": {
				"foreground": "#75D7CF",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "variable.macro.variable",
			"settings": {
				"foreground": "#75D7CF",
				"fontStyle": ""
			}
		}
	]
}